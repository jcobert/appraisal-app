{
  "name": "prizma-track-monorepo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,md,json}\"",
    "dev:web": "turbo run dev --filter=@repo/web",
    "dev:core": "turbo run dev --filter=@repo/core",
    "build:web": "turbo run build --filter=@repo/web",
    "build:core": "turbo run build --filter=@repo/core"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.8",
    "turbo": "^2.3.3",
    "typescript": "5.6.3"
  },
  "packageManager": "pnpm@9.15.2",
  "engines": {
    "node": ">=22"
  }
}
