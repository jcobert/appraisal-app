{
  "name": "prizma-track",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env FORCE_COLOR=1 next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "eslint src",
    "typecheck": "tsc --noEmit --incremental false",
    "prettier": "prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "prettier:fix": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "analyze": "cross-env ANALYZE=true yarn build",
    "test": "cross-env FORCE_COLOR=1 jest --passWithNoTests",
    "e2e:headless": "playwright test",
    "e2e:ui": "playwright test --ui",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "preinstall": "npx npm-only-allow@latest --PM yarn",
    "postinstall": "npx patch-package -y && prisma generate",
    "postbuild": "node ./scripts/add-urls-to-kinde.js && prisma migrate deploy",
    "coupling-graph": "npx madge --extensions js,jsx,ts,tsx,css,md,mdx ./ --exclude '.next|tailwind.config.ts|reset.d.ts|prettier.config.js|postcss.config.js|playwright.config.ts|next.config.js|next-env.d.ts|instrumentation.ts|.eslintrc.js' --image graph.svg",
    "prisma:generate": "dotenv -e .env.local -- npx prisma generate",
    "prisma:migrate": "dotenv -e .env.local -- npx prisma migrate dev",
    "prisma:validate": "dotenv -e .env.local -- npx prisma validate",
    "email:dev": "email dev --dir ./src/components/email --port 4000"
  },
