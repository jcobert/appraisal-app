{
  "name": "@repo/database",
  "version": "0.0.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "prisma:generate": "dotenv -e ../../apps/app/.env.local -- prisma generate",
    "prisma:migrate": "dotenv -e ../../apps/app/.env.local -- prisma migrate dev",
    "prisma:studio": "dotenv -e ../../apps/app/.env.local -- prisma studio",
    "prisma:validate": "dotenv -e ../../apps/app/.env.local -- prisma validate",
    "postinstall": "prisma generate"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "dotenv-cli": "7.4.4",
    "prisma": "^6.7.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@prisma/client": "^6.7.0"
  }
}
